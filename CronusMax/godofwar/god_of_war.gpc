combo cb2 {
    // first run
    set_val(PS4_L3, +100);
    set_val(PS4_LX, +35);
    set_val(PS4_LY, -100);
    wait(8000);
    set_val(PS4_CIRCLE, +100);
    wait(500);
    wait(dooropenTime);
    
    // second run
    set_val(PS4_L3, +100);
    set_val(PS4_LX, +0);
    set_val(PS4_LY, -100);
    wait(6000);
    set_val(PS4_CIRCLE, +100);
    wait(500);
    wait(dooropenTime);
    
    // accept scraps
    wait(4000);
    set_val(PS4_CIRCLE, +100);
    wait(500);
    wait(2000);
    // exit
    set_val(PS4_OPTIONS, +100);
    wait(500);
    wait(1500);  
    set_val(PS4_TRIANGLE, +100);
    wait(500);
    wait(1500);
    set_val(PS4_CIRCLE, +100);
    wait(500);
    wait(2500);
    set_val(PS4_CIRCLE, +100);
    wait(500);
    wait(500);
    wait(10000);
}



