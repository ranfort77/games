// 뉴게임 플러스에서 
// 무스펠하임 또는 니플하임에 첫 방문시
// 흡착용제와 xp (돈으로 변환 가능)를 얻고
// 체크포인트에서 다시 시작하면 또 다시 반복적으로 
// 얻을 수 있는 것을 이용한 노가다.

int dooropenTime = 6000; 
int switch = FALSE;

main {
    if (switch) {
        combo_run(maxcmb);
    } else  {   
        combo_stop(maxcmb);
    }
    if (event_press(PS4_L2)) {
        combo_run(cb_onoff);
    }
}


combo cb_onoff {
    switch = !switch;
}

